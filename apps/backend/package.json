{
  "name": "fsa-backend",
  "version": "1.0.0",
  "description": "FSA Backend API Service",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc && cp -r src/lib dist/",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node setup-initial-data.js",
    "migrate:personnel": "node migrate-personnel.js",
    "migrate:statuses": "node migrate-statuses.js",
    "seed:personnel": "tsx temp-personnel-seed.ts",
    "db:optimize": "node ../../scripts/optimize-database-indexes.js",
    "db:monitor": "node ../../scripts/monitor-database-scaling.js",
    "db:capacity": "node ../../scripts/calculate-database-capacity.js",
    "db:health": "node ../../scripts/monitor-database-health.js",
    "db:analyze": "npm run db:monitor && npm run db:optimize",
    "db:full-analysis": "npm run db:capacity && npm run db:monitor && npm run db:optimize",
    "db:check": "npm run db:health"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/static": "^8.2.0",
    "@fastify/websocket": "^11.2.0",
    "@types/pdfkit": "^0.17.3",
    "@types/socket.io": "^3.0.2",
    "bcryptjs": "^3.0.2",
    "dateformat": "^3.0.3",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.17.2",
    "readline-sync": "^1.4.10",
    "socket.io": "^4.8.1",
    "stripe": "^18.5.0",
    "sync-request": "^5.0.0",
    "text-encoding": "^0.6.4",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
